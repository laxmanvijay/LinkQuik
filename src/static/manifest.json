{
    "manifest_version": 3,
    "name": "LinkQuik",
    "description": "A chrome extension for connecting to LinkedIn people faster",
    "version": "1.0.0",
    "action" : {
        "default_popup": "popup.html",
        "default_title": "React Boilerplate",
        "default_icon" : "linkedin.png"
    },
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "16" : "linkedin.png",
        "48" : "linkedin.png",
        "128" : "linkedin.png"
    },
    "content_scripts": [
        {
            "matches": ["*://*.linkedin.com/*"],
            "js": ["contentScript.js"]
        }
    ]
}